<div class="row">
  <div class="col-md-6">
    <%= notice %>
    <%= form_tag generate_token_path, method: :post, class: "navbar-form navbar-left" do %>
      <%= text_field_tag(:email, params[:email], placeholder: "user@test.com", class: "form-control") %>
      <%= submit_tag 'Genere Api Key', class: "btn btn-default" %>
    <% end %>
  </div>
  <div class="col-md-6">
    <% if !@users.blank? %>
      <p>List of keys </p>
      <table>
        <thead>
          <tr>
          <td><p> Token: </p></td>
          <td ><p> Email: </p></td>
          <td><p> Counter: </p></td>
        </tr>
        </thead>
      <% @users.each  do  |user| %>
      <tbody>
        <tr>
          <td WIDTH="300"><p> <%= user.token %> </p></td>
          <td><p> <%= user.email %> </p></td>
          <td><p> <%= user.counter %> </p></td>
          <td> <%= link_to "Find Duplicates", find_duplicates_path(:token => user.token, data: ['hola', 'parso', 'mundo', 'hola', 'agua', 'papa', 'parso']), :method => :get %></td>
        </tr>
      </tbody>
      <% end %>
      </table>
    <% end %>
  </div>
</div>
